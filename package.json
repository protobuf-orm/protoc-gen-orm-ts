{
	"name": "@protobuf-orm/runtime",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -p ./tsconfig.build.json && vite build",
		"check": "biome check --write",
		"test": "vitest"
	},
	"workspaces": [
		"examples"
	],
	"exports": {
