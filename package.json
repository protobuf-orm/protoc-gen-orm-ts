		"@types/node": "^24.10.0",
		"fake-indexeddb": "^6.2.4",
		"tsx": "^4.20.6",
		"vite": "^7.1.12",
		"vite-plugin-dts": "^4.5.4",
		"vitest": "^4.0.5",
		"zx": "^8.8.5"
	},
	"dependencies": {
		"@connectrpc/connect": "^2.1.0",
		"@protobuf-orm/runtime": "file:protobuf-orm-runtime-0.0.1.tgz"
